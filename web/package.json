{
  "name": "web",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "analyze:custom": "node scripts/bundle-analysis.js",
    "analyze:baseline": "node scripts/bundle-analysis.js --save-baseline",
    "analyze:detailed": "node scripts/bundle-analysis.js --detailed",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "build:analyze": "pnpm build && pnpm analyze:custom"
  },
  "dependencies": {
    "@bufbuild/protobuf": "2.6.3",
    "@connectrpc/connect": "2.0.4",
    "@connectrpc/connect-query": "2.1.1",
    "@connectrpc/connect-web": "2.0.4",
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "5.15.14",
    "@mui/material": "5.15.14",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-web": "0.49.0",
    "@opentelemetry/core": "1.25.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.57.2",
    "@opentelemetry/instrumentation": "0.57.2",
    "@opentelemetry/instrumentation-fetch": "0.57.2",
    "@opentelemetry/instrumentation-xml-http-request": "0.57.2",
    "@opentelemetry/resources": "1.25.1",
    "@opentelemetry/sdk-trace-web": "1.25.1",
    "@opentelemetry/semantic-conventions": "1.27.0",
    "i18next": "23.15.2",
    "next": "14.1.0",
    "next-i18next": "15.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.1.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.32.0",
    "@next/bundle-analyzer": "14.1.0",
    "@size-limit/preset-app": "11.1.6",
    "@size-limit/webpack": "11.1.6",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/cypress": "^1.1.6",
    "@types/jest": "29.5.14",
    "@types/node": "24.1.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "cypress": "^14.5.3",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.3",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.6.2",
    "size-limit": "11.1.6",
    "typescript": "5.2.2",
    "webpack-bundle-analyzer": "4.10.2"
  }
}
