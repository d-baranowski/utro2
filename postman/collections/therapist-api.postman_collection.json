      "name": "Therapist Service",
      "item": [
        {
          "name": "Get Therapist by ID",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status code is 200 or 404', function () {",
                  "    pm.expect(pm.response.code).to.be.oneOf([200, 404]);",
                  "});",
                  "",
                  "if (pm.response.code === 200) {",
                  "    pm.test('Response contains therapist data', function () {",
                  "        const jsonData = pm.response.json();",
                  "        pm.expect(jsonData).to.have.property('therapist');",
                  "        pm.expect(jsonData.therapist).to.have.property('id');",
                  "    });",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"id\": \"1\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/utro.v1.TherapistService/GetTherapist",
              "host": ["{{baseUrl}}"],
              "path": ["utro.v1.TherapistService", "GetTherapist"]
            }
